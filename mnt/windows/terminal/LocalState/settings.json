  "disabledProfileSources": ["Windows.Terminal.Azure"],
  // üëÄ https://docs.microsoft.com/ja-jp/windows/terminal/customize-settings/key-bindings
  "actions": [
    // unbind
    {
      "command": "unbound",
      "keys": "ctrl+,"
    },
    {
      "command": "unbound",
      "keys": "ctrl+v"
    },
    // Ë®≠ÂÆö
    {
      "command": "openSettings",
      "keys": "ctrl+shift+s"
    },
    // Âü∫Êú¨Êìç‰Ωú
    {
      "command": {
        "action": "copy",
        "singleLine": false
      },
      "keys": "ctrl+c"
    },
    {
      "command": "find",
      "keys": "ctrl+alt+f"
    },
    {
      "command": "commandPalette",
      "keys": "ctrl+shift+p"
    },
    // ÁîªÈù¢„Çπ„ÇØ„É≠„Éº„É´
    {
      "command": "scrollUp",
      "keys": "shift+up"
    },
    {
      "command": "scrollDown",
      "keys": "shift+down"
    },
    {
      "command": "scrollUpPage",
      "keys": "shift+pgup"
    },
    {
      "command": "scrollDownPage",
      "keys": "shift+pgdn"
    },
    // „Çø„Éñ
    {
      "command": "newTab",
      "keys": "ctrl+t"
    },
    {
      "command": "closeTab",
      "keys": "alt+w"
    },
    {
      "command": "nextTab",
      "keys": "alt+l"
    },
    {
      "command": "prevTab",
      "keys": "alt+h"
    },
    // „Éö„Ç§„É≥„ÅÆÂàÜÂâ≤
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "keys": "alt+shift+d"
    },
    // „Éö„Ç§„É≥ÁßªÂãï
    {
      "command": {
        "action": "moveFocus",
        "direction": "down"
      },
      "keys": "alt+ctrl+j"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "left"
      },
      "keys": "alt+ctrl+h"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "right"
      },
      "keys": "alt+ctrl+l"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "up"
      },
      "keys": "alt+ctrl+k"
    },
    // „Éö„Ç§„É≥„Çµ„Ç§„Ç∫Â§âÊõ¥
    {
      "command": {
        "action": "resizePane",
        "direction": "down"
      },
      "keys": "alt+shift+j"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "left"
      },
      "keys": "alt+shift+h"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "right"
      },
      "keys": "alt+shift+l"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "up"
      },
      "keys": "alt+shift+k"
    }
