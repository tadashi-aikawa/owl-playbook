{
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#777777",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "\n {{ .Path }} ",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#6CA35E",
          "style": "plain",
          "template": " 󰎙 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "type": "node"
        },
        {
          "foreground": "#96E072",
          "style": "plain",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "type": "python"
        },
        {
          "foreground": "#7FD5EA",
          "style": "plain",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "type": "go"
        },
        {
          "foreground": "#E06C75",
          "style": "plain",
          "template": "!",
          "type": "root"
        },
        {
          "foreground": "#96E072",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb3b{{ end }}",
            "{{ if gt .Ahead 0 }}#8A4FFF{{ end }}",
            "{{ if gt .Behind 0 }}#c42ebd{{ end }}"
          ],
          "properties": {
            "branch_icon": "  ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "{{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }}   <#E84855>{{ .Working.String }}</>{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} 󰆼 <#ffeb3b>{{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0}} 󰽄  {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}}  {{ .WorktreeCount }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground_templates": [
            "{{ if gt .Code 0 }}#e91e63{{ else }}#777777{{ end }}"
          ],
          "style": "plain",
          "template": " 󰏒 {{ if gt .Code 0 }}❯❯❯{{ else }}❯{{ end }}",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
