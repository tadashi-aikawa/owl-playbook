{
  "editor.fontSize": 16,
  // "editor.fontFamily": "Ricty Diminished",
  "editor.fontFamily": "Source Code Pro for Powerline",
  "workbench.colorTheme": "Monokai",
  "files.eol": "\n",
  "explorer.confirmDragAndDrop": false,
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.visualstar": true,
  "vim.leader": "<space>",
  // "vim.sneak": true,
  "vim.easymotion": true,
  "vim.easymotionMarkerBackgroundColor": "rgba(0, 0, 0, 0.7)",
  "vim.easymotionMarkerForegroundColorOneChar": "pink",
  "vim.easymotionMarkerWidthPerChar": 12,
  "vim.easymotionMarkerHeight": 24,
  "vim.easymotionMarkerFontSize": "24",
  "vim.easymotionMarkerYOffset": 5,
  "vim.enableNeovim": true,
  "vim.neovimPath": "C:\\tools\\neovim\\Neovim\\bin\\nvim.exe",
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<C-k>"
      ],
      "after": [
        "g",
        "b"
      ]
    },
    {
      "before": [
        "<C-o>"
      ],
      "commands": [
        {
          "command": "workbench.action.navigateBack"
        }
      ]
    },
    {
      "before": [
        "<C-i>"
      ],
      "commands": [
        {
          "command": "workbench.action.navigateForward"
        }
      ]
    },
    {
      "before": [
        "s"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "2",
        "s",
        "<char>",
        "<char>"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        "<C-k>",
      ],
      "after": [
        "g",
        "b"
      ]
    }
  ],
  "editor.minimap.showSlider": "always",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": true,
  "editor.tabCompletion": "on",
  "editor.snippetSuggestions": "top",
  "editWithShell.shell.windows": "bash",
  "editWithShell.shellArgs.windows": [
    "-c"
  ],
  "editWithShell.processEntireTextIfNoneSelected": true,
  "vim.disableExtension": false,
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#3fd6fc79",
    "editor.selectionHighlightBackground": "#3fd6fc79",
  },
  "breadcrumbs.enabled": true,
  "window.zoomLevel": 0,
  "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
  "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
  "window.title": "${dirty}${activeEditorMedium} [${rootName}]",
  "terminal.integrated.rendererType": "dom",
  "explorer.confirmDelete": false,
  "editor.formatOnPaste": true,
  "vsnotes.defaultNotePath": "~\\backup\\notes",
  "files.associations": {
    "Vagrantfile": "ruby"
  },
  "editor.multiCursorModifier": "ctrlCmd",
  "[plaintext]": {
    "files.autoGuessEncoding": true,
  },
  "workbench.editor.showTabs": true,
  "vim.foldfix": true,
  "git.autofetch": true,
  "go.useLanguageServer": true,
  "python.formatting.provider": "black",
  "editor.formatOnSave": true,
  "cmake.configureOnOpen": true,
  "git.ignoreLegacyWarning": true,
  "workbench.editor.enablePreview": false,
}