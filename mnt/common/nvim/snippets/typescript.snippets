    // deno-fmt-ignore
    // prettier-ignore
    //   ${2:args1}         | ${3:args2}     | expected
    [
      [  "args1"       , "args2"   , "expected" ],
      [  "args1"       , "args2"   , "expected" ],
    ] as const,
