snippet _deno_parameterized_test "Deno„ÅÆParameterized test" b
  export function parameterizedTest<T>(
    name: string,
    cases: T[],
    testFn: (input: T) => void,
    toTitle?: (name: string, input: T) => string,
  ) {
    for (const testCase of cases) {
      Deno.test(
        toTitle?.(name, testCase) ?? \`\${name} -> \${JSON.stringify(testCase)}\`,
        () => {
          testFn(testCase);
        },
      );
    }
  }

snippet _deno_parameterized_testcase "Deno„ÅÆParameterized test„ÅÆ„ÉÜ„Çπ„Éà„Ç±„Éº„Çπ" b
