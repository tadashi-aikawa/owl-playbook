extends silhouette

snippet _
    - [ ] ${1}

snippet _mtg
    - [ ] ${1:10}:${2:00} [[MTG_${4:yyyymmdd}_${3:MTG名}]]

snippet _note "[Alert] Note"
    > [!NOTE]
    > ${1}

snippet _tip "[Alert] TIP"
    > [!TIP]
    > ${1}

snippet _important "[Alert] IMPORTANT"
