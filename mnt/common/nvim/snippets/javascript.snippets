snippet _deno_parameterized_test "Denoã®Parameterized test" b
  export function parameterizedTest(name, cases, testFn, toTitle) {
    for (const testCase of cases) {
      Deno.test(
        toTitle?.(name, testCase) ?? \`\${name} -> \${JSON.stringify(testCase)}\`,
        () => {
          testFn(testCase);
        },
      );
    }
  }

