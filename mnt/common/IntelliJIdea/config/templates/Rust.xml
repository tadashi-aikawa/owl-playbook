<templateSet group="Rust">
  <template name="st" value="struct $NAME$ {&#10;    $END$&#10;}" description="struct" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="RUST_MOD" value="true" />
      <option name="RUST_STATEMENT" value="true" />
    </context>
  </template>
  <template name="l" value="let $NAME$ = $END$;" description="let ? =" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="RUST_STATEMENT" value="true" />
    </context>
  </template>
  <template name="sf" value="String::from(&quot;$END$&quot;)" description="String::from(&quot;&quot;)" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="RUST_FILE" value="true" />
      <option name="RUST_ITEM" value="false" />
    </context>
  </template>
  <template name="debug" value="#[derive(Debug)]" description="#[derive(Debug)]" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="RUST_MOD" value="true" />
      <option name="RUST_STATEMENT" value="true" />
    </context>
  </template>
  <template name="unittest" value="#[cfg(test)]&#10;mod tests {&#10;    use super::*;&#10;&#10;    #[test]&#10;    fn this_test_will_pass() {&#10;        assert!(true);&#10;    }&#10;}&#10;" description="unittest" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="RUST_MOD" value="true" />
    </context>
  </template>
  <template name="derive" value="#[derive($END$)]" description="#[derive()]" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="RUST_MOD" value="true" />
      <option name="RUST_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>