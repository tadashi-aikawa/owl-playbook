endsnippet

#-------------------------------------------------
# Jumeaux
#-------------------------------------------------
snippet jumeaux_release
## :package: ${1:x.y.z}

:fa-calendar: \``!v strftime("%Y-%m-%d")`\`

$0
endsnippet

snippet jumeaux_breaking_changes
??? danger "${1:Summary} ([${2}])"

    ${3:detail}
endsnippet

snippet jumeaux_improvement
??? hint "${1:Summary} ([${2}])"

    ${3:detail}
endsnippet

snippet jumeaux_new_features
??? info "${1:Summary} ([${2}])"

    ${3:detail}
endsnippet

snippet jumeaux_bugfix
??? bug "${1:Summary}な不具合を修正しました ([${2}])"

    ${3:detail}
endsnippet

snippet jumeaux_other
??? note "${1:Summary} ([${2}])"

    ${3:detail}
endsnippet


