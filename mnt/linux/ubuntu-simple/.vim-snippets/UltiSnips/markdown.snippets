snippet info "ブログの情報"
{{<alert "info">}}
$0
{{</alert>}}
endsnippet

snippet hint "ブログのヒント"
{{<alert "success">}}
$0
{{</alert>}}
endsnippet

snippet warn "ブログの注意"
{{<alert "warning">}}
$0
{{</alert>}}
endsnippet

snippet danger "ブログの警告"
{{<alert "danger">}}
$0
{{</alert>}}
endsnippet


snippet file "ブログの折りたたみファイル/コード"
{{<file "${1:Summary}">}}
${2:Detail}
{{</file>}}
$0
endsnippet

snippet info "付随情報"
{{<info "${1:summary}">}}
${2:Detail}
{{</info>}}
$0
endsnippet

snippet why "疑問に対する回答"
{{<why "${1:summary}">}}
${2:Detail}
{{</why>}}
$0
endsnippet

snippet trouble "よくあるトラブルの対処法"
{{<warn "${1:summary}">}}
${2:Detail}
{{</warn>}}
$0
endsnippet

snippet error "Errorの対処法"
{{<error "${1:summary}">}}
${2:Detail}
{{</error>}}
$0
endsnippet

snippet refer "ブログの参考リンク"
{{<refer "${1:Title}" "${2:URL}">}}
$0
endsnippet


snippet no-icon "ブログのその他情報"
{{<alert "no-icon">}}
${2:Detail}
{{</alert>}}
$0
endsnippet

snippet summary "ブログのsummary"
